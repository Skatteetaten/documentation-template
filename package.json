{
  "name": "documentation-template",
  "version": "1.0.0",
  "description": "Aurora documentation-template",
  "license": "Apache-2.0",
  "scripts": {
    "prestart": "npm run clean",
    "start": "gatsby develop --verbose",
    "prebuild": "npm run clean",
    "build": "gatsby build --prefix-paths",
    "deploy": "gh-pages -d public -p 0",
    "prettier:fix": "prettier --write '{src,docs}/**/*.{js,jsx,css,md}'",
    "test": "echo \"Error: no test specified\"",
    "clean": "rimraf public .cache"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.3",
    "rimraf": "3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@skatteetaten/frontend-components": "5.0.6",
    "classnames": "^2.3.1",
    "gatsby": "2.25",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "2.3.12",
    "gatsby-remark-prismjs": "3.5.11",
    "gatsby-source-filesystem": "2.3.27",
    "gatsby-transformer-remark": "2.8.31",
    "github-slugger": "^1.4.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "rehype-react": "5.0.1",
    "slash": "3.0.0"
  }
}
