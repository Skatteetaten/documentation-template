{
  "name": "documentation-template",
  "groupId": "no.skatteetaten.aurora",
  "version": "1.0.0",
  "description": "Aurora documentation-template",
  "license": "Apache-2.0",
  "files": [
    "metadata",
    "public"
  ],
  "scripts": {
    "prestart": "npm run clean",
    "start": "gatsby develop --verbose",
    "prepare": "husky install",
    "prebuild": "npm run clean",
    "build": "gatsby build --prefix-paths",
    "build:local": "gatsby build",
    "deploy": "gh-pages -d public -p 0",
    "prettier:fix": "prettier --write '{src,docs}/**/*.{js,jsx,css,md}'",
    "test": "echo \"no test specified\"",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@skatteetaten/frontend-components": "5.0.6",
    "classnames": "2.3.1",
    "gatsby": "4.21.1",
    "gatsby-plugin-react-helmet": "5.21.0",
    "gatsby-remark-autolink-headers": "5.21.0",
    "gatsby-remark-copy-linked-files": "5.21.0",
    "gatsby-remark-prismjs": "6.21.0",
    "gatsby-source-filesystem": "4.21.1",
    "gatsby-transformer-remark": "5.21.0",
    "github-slugger": "1.4.0",
    "lodash": "4.17.21",
    "prismjs": "1.29.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "rehype-react": "7.1.1",
    "slash": "4.0.0"
  },
  "devDependencies": {
    "@types/react-helmet": "6.1.5",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "prettier": {
    "singleQuote": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
